<% if @work_record.errors.present? %>
	$("div#myModal").html("<%= j(render(:partial => 'njsform')) %>");
<% else %>
	$("div#myModal").modal('hide');
	$("tbody#work_records").append("<tr id='wr_<%= @work_record.id %>'><%= j(render(:partial => 'work_record',:object => @work_record)) %></tr>");
<% end -%>